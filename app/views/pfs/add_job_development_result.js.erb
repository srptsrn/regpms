<% if @error_message.blank? %>
	$('#job-development-result-list-<%= @job_development.id %>').append("<%=j render(partial: "row_job_development_result", locals: {job_development_result: @job_development_result}) %>");
	$('#new_job_development_result_<%= @job_development.id %>_name').val('');
	$('#new_job_development_result_<%= @job_development.id %>_job_result_template_id').val('');
	$('#new_job_development_result_<%= @job_development.id %>_job_result_template_id').trigger('chosen:updated');
	$('#job-development-result-row-<%= @job_development_result.id %>').effect("highlight", 1000);
<% else %>
	alert('<%=h @error_message %>');
<% end %>

init_input_chosen();