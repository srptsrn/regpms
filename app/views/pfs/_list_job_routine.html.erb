<table class="table">
	<thead>
		<tr>
			<th><%= JobRoutine.human_attribute_name(:job_template) %></th>
			<th style="width:100px; text-align:right;"><%= JobRoutine.human_attribute_name(:expect) %></th>
			<th style="width:100px; text-align:right;"><%= JobRoutine.human_attribute_name(:weight) %></th>
			<th colspan="2" style="width:100px; text-align:right;"><%= JobRoutine.human_attribute_name(:self_point) %></th>
		</tr>
	</thead>
	<tbody id="job-routine-list">
		<% current_user.job_routines.where(["evaluation_id = ?", @current_evaluation.id]).sort_by {|jj| [jj.created_at]}.each do |job_routine| %>
			<%= render partial: "row_job_routine", locals: {job_routine: job_routine} %>
		<% end %>
	</tbody>
</table>
<div class="clearfix"></div>