<% if @error_message.blank? %>

	$('#job-development-row-<%= @job_development.id %>').replaceWith("<%=j render(partial: "row_job_development_form", locals: {job_development: @job_development}) %>");
	$('#job-development-row-<%= @job_development.id %>').effect("highlight");
	
<% else %>
	alert('<%=h @error_message %>');
<% end %>
