<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :evaluation %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :percent_of_change %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :total_amount %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :total_eligible_amount %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :point_level1 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :point_level2 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :point_level3 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :point_level4 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :point_level5 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :min_change1 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :min_change2 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :min_change3 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :min_change4 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :min_change5 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :max_change1 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :max_change2 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :max_change3 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :max_change4 %></th>
            <th nowrap><%= EvaluationSalaryUp.human_attribute_name :max_change5 %></th>
            <% unless action_name == "show" %>
                <th style="width:30px;">
                    <%= link_to_add_association f, :evaluation_salary_ups, partial: "/evaluation_salary_ups/nested_fields", class: "btn btn-sm btn-success",
                    "data-association-insertion-node" => "#evaluation_salary_up_fields",
                    "data-association-insertion-method" => :append do %>
                    <i class='fa fa-plus'></i>
                    <% end %>
                </th>
            <% end %>
        </tr>
    </thead>
    <tbody id="evaluation_salary_up_fields">
        <%= f.simple_fields_for :evaluation_salary_ups do |evaluation_salary_up| %>
            <%= render '/evaluation_salary_ups/nested_fields', :f => evaluation_salary_up %>
        <% end %>
    </tbody>
</table>
<script type="text/javascript">
    jQuery(function($) {
        $('#evaluation_salary_up_fields').on('cocoon:after-insert', function(e, insertedItem) {
            init_input_date_picker();
            init_input_time_picker();
            init_input_datetime_picker();
        });
    });
</script>