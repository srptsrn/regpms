<% if @error_message.blank? %>
	$('#job-routine-result-list-<%= @job_routine.id %>').append("<%=j render(partial: "row_job_routine_result", locals: {job_routine_result: @job_routine_result}) %>");
	$('#new_job_routine_result_<%= @job_routine.id %>_name').val('');
	$('#new_job_routine_result_<%= @job_routine.id %>_job_result_template_id').val('');
	$('#new_job_routine_result_<%= @job_routine.id %>_job_result_template_id').trigger('chosen:updated');
	$('#job-routine-result-row-<%= @job_routine_result.id %>').effect("highlight", 1000);
<% else %>
	alert('<%=h @error_message %>');
<% end %>

init_input_chosen();