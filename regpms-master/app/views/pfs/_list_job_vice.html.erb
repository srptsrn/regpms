<table class="table">
	<thead>
		<tr>
			<th><%= JobVice.human_attribute_name(:job_template) %></th>
			<th style="width:100px; text-align:right;"><%= JobVice.human_attribute_name(:expect) %></th>
			<th style="width:100px; text-align:right;"><%= JobVice.human_attribute_name(:weight) %></th>
			<th colspan="2" style="width:100px; text-align:right;"><%= JobVice.human_attribute_name(:self_point) %></th>
		</tr>
	</thead>
	<tbody id="job-vice-list">
		<% current_user.job_vices.where(["evaluation_id = ?", @current_evaluation.id]).sort_by {|jj| [jj.created_at]}.each do |job_vice| %>
			<%= render partial: "row_job_vice", locals: {job_vice: job_vice} %>
		<% end %>
	</tbody>
</table>
<div class="clearfix"></div>