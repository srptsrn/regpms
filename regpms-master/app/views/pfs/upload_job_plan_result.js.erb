<% if @error_message.blank? %>
	$('#job-plan-result-files-<%= @job_plan_result.id %>').append("<%=j render(partial: "row_job_plan_result_file", locals: {job_plan_result_file: @job_plan_result_file}) %>");
	$('#new_job_plan_result_file_<%= @job_plan_result.id %>_file').val('');
	$('#new_job_plan_result_file_<%= @job_plan_result.id %>_description').val('');
	$('#job-plan-result-file-<%= @job_plan_result_file.id %>').effect("slide", 1000);
<% else %>
	alert('<%=h @error_message %>');
<% end %>
