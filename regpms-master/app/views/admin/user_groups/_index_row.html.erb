<% if can?(:index_select, UserGroup) %>
    <td class="center">
	 	<label class="csscheckbox csscheckbox-default">
            <%= check_box_tag 'ids[]', user_group.id, false %>
            <span></span>
        </label>
    </td>
<% end %>
<td nowrap>
    <%= render partial: "/appui_scaffold/index_row_actions", locals: {
        resource_class: UserGroup, resource: user_group,
        resources_url: admin_user_groups_path(q: params[:q], page: params[:page]),
        resource_url: admin_user_group_path(user_group, q: params[:q], page: params[:page]),
        edit_resource_url: edit_admin_user_group_path(user_group, q: params[:q], page: params[:page])
    } %>
</td>




<td><%= user_group.name %></td>


